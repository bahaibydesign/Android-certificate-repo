fun main(){

  val age = 5 * 365
  val name ="rover"
  println("Happy Birthday, ${name}!")

  // Let's print a cake!
    println("   ,,,,,   ")
    println("   |||||   ")
    println(" =========")
    println("@@@@@@@@@@@")
    println("{~@~@~@~@~}")
    println("@@@@@@@@@@@")

    // this prints an empty line
    println("")

    println("You are already ${age} days old, ${name}!")
    println("${age} is the very best age to celebrate!")
}
